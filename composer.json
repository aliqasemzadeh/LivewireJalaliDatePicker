{
    "name": "aliqasemzadeh/livewire-jalali-date-picker",
    "description": "A Livewire component for Jalali (Persian) date picker with AlpineJS",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Qasemzadeh",
            "email": "ali.qasemzadeh@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "livewire/livewire": "^2.0|^3.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Aliqasemzadeh\\LivewireJalaliDatePicker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aliqasemzadeh\\LivewireJalaliDatePicker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aliqasemzadeh\\LivewireJalaliDatePicker\\LivewireJalaliDatePickerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
